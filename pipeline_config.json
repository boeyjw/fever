{
    "__variables":
    {
        "___root_dir___":"/home/tyoneda/pipeline",
        "___model_name___": "pre_trained",
        "___reader___": "esim_pre_trained_appendtitle+"
    },
    "__model_description":
    {
        "data": "pre_trained model",
        "feature": "usint model pre-trained on SNLI"
    },
    "config":{
        "save_dir":"___root_dir___/fever/results/___model_name___/config.json"
    },
    "ir":
    {
        "__train_output_file": "___root_dir___/fever/data/indexed_data/train.sentences.p10.s30.jsonl",
        "__dev_output_file": "___root_dir___/fever/data/indexed_data/dev.sentences.p10.s30.jsonl",
        "train_output_file": "___root_dir___/fever/data/indexed_data/train.sentences.p5.s5.ver20180629.jsonl",
        "dev_output_file": "___root_dir___/fever/data/indexed_data/dev.sentences.p5.s5.ver20180629.jsonl",
        "train_input_file": "___root_dir___/fever/data/train.jsonl",
        "dev_input_file": "___root_dir___/fever/data/dev.jsonl",
        "__train_output_file": "___root_dir___/fever/data/indexed_data/train.shared_task_data.jsonl",
        "__dev_output_file": "___root_dir___/fever/data/indexed_data/dev.shared_task_data.jsonl",
        "n_pages": 5,
        "n_sentences": 5
    },
    "convert":
    {
        "__comment": "not using now",
        "prependtitle": true,
        "prependlinum": false,
        "use_ir_pred": false,
        "n_sentences": 5,
        "train_converted_file": "not using now ___root_dir___/fever/data/converted/train.sentences.use_ir_pred_filtered.p5.s5.jsonl",
        "dev_converted_file": "not using now ___root_dir___/fever/data/converted/dev.sentences.use_ir_pred_filtered.p5.s5.jsonl"
    },
    "train_rte":
    {
        "save_dir": "___root_dir___/jack/___reader___",
        "train_file": {"__overwrite": "convert train_converted_file"},
        "dev_file": {"__overwrite": "convert dev_converted_file"}
    },
    "inference_rte":
    {
        "train_input_file": {"__overwrite": "ir train_output_file"},
        "dev_input_file": {"__overwrite": "ir dev_output_file"},
        "saved_reader": {"__overwrite": "train_rte save_dir"},
        "prependtitle": {"__overwrite": "convert prependtitle"},
        "prependlinum": {"__overwrite": "convert prependlinum"},
        "n_sentences": 5,
        "train_predicted_labels_and_scores_file": "___root_dir___/fever/results/___model_name___/train.predictions.jsonl",
        "dev_predicted_labels_and_scores_file": "___root_dir___/fever/results/___model_name___/dev.predictions.jsonl"
    },
    "aggregator":
    {
        "train_file": {"__overwrite": "inference_rte train_predicted_labels_and_scores_file"},
        "dev_file": {"__overwrite": "inference_rte dev_predicted_labels_and_scores_file"},
        "layers": [15, 10, 5],
        "epochs": 5,
        "n_sentences": {"__overwrite": "inference_rte n_sentences"},
        "predicted_labels_file": "___root_dir___/fever/results/___model_name___/aggregated_labels.jsonl"
    },
    "score":
    {
        "predicted_labels_file": {"__overwrite": "aggregator predicted_labels_file"},
        "predicted_evidence_file": {"__overwrite": "ir dev_output_file"},
        "actual_file": {"__overwrite": "ir dev_input_file"},
        "score_file": "___root_dir___/fever/results/___model_name___/score.jsonl"
    },
    "_additional_comment":
    {
    }
